{
  "name": "magento/project-community-edition",
  "description": "eCommerce Platform for Growth (Community Edition)",
  "type": "project",
  "version": "2.1.2",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
    "magento/product-community-edition": "2.1.2",
    "composer/composer": "@alpha"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.0",
    "squizlabs/php_codesniffer": "1.5.3",
    "phpmd/phpmd": "@stable",
    "pdepend/pdepend": "2.2.2",
    "fabpot/php-cs-fixer": "~1.2",
    "lusitanian/oauth": "~0.3 <=0.7.0",
    "sebastian/phpcpd": "2.0.0"
  },
  "config": {
    "use-include-path": true
  },
  "autoload": {
    "psr-4": {
      "Magento\\Framework\\": "lib/internal/Magento/Framework/",
      "Magento\\Setup\\": "setup/src/Magento/Setup/",
      "Magento\\": "app/code/Magento/",
      "Mirasvit\\Core\\": "vendor/mirasvit/module-core/src/Core",
      "Mirasvit\\Misspell\\": "vendor/mirasvit/module-misspell/src/Misspell",
      "Mirasvit\\SearchAutocomplete\\": "vendor/mirasvit/module-search-autocomplete/src/SearchAutocomplete",
      "Mirasvit\\Search\\": "vendor/mirasvit/module-search-sphinx/src/Search",
      "Mirasvit\\SearchMysql\\": "vendor/mirasvit/module-search-sphinx/src/SearchMysql",
      "Mirasvit\\SearchSphinx\\": "vendor/mirasvit/module-search-sphinx/src/SearchSphinx",
      "Mirasvit\\SearchUltimate\\": "vendor/mirasvit/module-search-ultimate/src/SearchUltimate"
    },
    "psr-0": {
      "": "app/code/"
    },
    "files": [
      "app/etc/NonComposerComponentRegistration.php",
      "vendor/mirasvit/module-core/src/Core/registration.php",
      "vendor/mirasvit/module-misspell/src/Misspell/registration.php",
      "vendor/mirasvit/module-search-autocomplete/src/SearchAutocomplete/registration.php",
      "vendor/mirasvit/module-search-sphinx/src/Search/registration.php",
      "vendor/mirasvit/module-search-sphinx/src/SearchMysql/registration.php",
      "vendor/mirasvit/module-search-sphinx/src/SearchSphinx/registration.php",
      "vendor/mirasvit/module-search-ultimate/src/SearchUltimate/registration.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
      "Magento\\Tools\\": "dev/tools/Magento/Tools/",
      "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
      "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
      "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
    }
  },
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "extra": {
    "magento-force": "override"
  }
}